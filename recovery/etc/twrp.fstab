/recovery		emmc	/dev/block/mmcblk0p22
/boot			emmc	/dev/block/mmcblk0p21
/misc			emmc	/dev/block/mmcblk0p23
/cache			ext4	/dev/block/mmcblk0p37
/data			ext4	/dev/block/mmcblk0p38
/system			ext4	/dev/block/mmcblk0p36
/sdcard			vfat	/dev/block/mmcblk0p39	flags=display="Internal Storage";storage;
/external_sd	vfat	/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SDcard";storage;removable;
/usb-otg		vfat	/dev/block/sda1			/dev/block/sda		flags=display="USB Memory";storage;removable
